<!doctype html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Example App</title>
    <script src="bower_components/jquery/dist/jquery.min.js"></script>
    <script src="bower_components/jquery-ui/jquery-ui.min.js"></script>
    <link rel="stylesheet" href="bower_components/bootstrap/dist/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <script src="bower_components/bootstrap/dist/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
    {{#if layoutData.scripts}}
      {{#each layoutData.scripts}}
        <script src="{{script}}"></script>
      {{/each}}
    {{/if}}
    {{#if layoutData.styles}}
      {{#each layoutData.styles}}
        <link rel="stylesheet" href="{{script}}"></link>
      {{/each}}
    {{/if}}
</head>
<body>
    {{{body}}}
</body>
</html>
